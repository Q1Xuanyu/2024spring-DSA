   "source": [
    "import math\n",
    "\n",
    "s=input()\n",
    "n=len(s)\n",
    "m=math.floor(math.log2(n))\n",
    "\n",
    "key=''\n",
    "for i in range(0,m+1):\n",
    "    a,b=2**i-1,2**(m-i)-1\n",
    "    if a>b:\n",
    "        break\n",
    "    elif a==b:\n",
    "        key+=s[a]\n",
    "    else:\n",
    "        key+=s[a]\n",
    "        key+=s[b]\n",
    "\n",
    "print(key)"
   ]
  }
 ],
